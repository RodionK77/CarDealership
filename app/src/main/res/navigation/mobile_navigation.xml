<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.cardealership.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.cardealership.ui.search.SearchFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.cardealership.ui.account.UserFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_user" />
    <fragment
        android:id="@+id/administrator"
        android:name="com.example.cardealership.ui.registration.RegistrationAdminFragment"
        android:label="Administrator" />
    <fragment
        android:id="@+id/simpleUser"
        android:name="com.example.cardealership.ui.registration.RegistrationUserFragment"
        android:label="SimpleUser" />
</navigation>